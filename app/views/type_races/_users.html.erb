<table class="table table-striped table-bordered">
  <thead >
  <tr>
    <th> User Name</th>
    <th >Email</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  </thead>
  <div class="users" id="hidden">
    <tbody >
    <% @type_race.users.each do |user| %>
      <tr>
        <td id="userName"> <%= user.user_name %></td>
        <td id="userEmail"> <%= user.email %> </td>
        <td id="userFirstName"> <%=  user.first_name %></td>
        <td id="userLastName">  <%= user.last_name %></td>
      </tr>
    <% end %>
  </div>
  </tbody>
</table>